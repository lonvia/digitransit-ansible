{
  "routingDefaults": {
    "transferSlack": 100,
    "waitReluctance": 2,
    "walkReluctance": 4,
    "stairsReluctance": 5,
    "walkBoardCost": 540,
    "numItineraries": 5,
    "bikeTriangleSafetyFactor": 0.4,
    "bikeTriangleSlopeFactor": 0.3,
    "bikeTriangleTimeFactor": 0.3,
    "vehicleRental" : {
      "pickupTime": 180,
      "pickupCost": 850,
      "bannedNetworks": ["car-sharing", "taxi", "cargo-bike"],
      "keepingAtDestinationCost": 180,
      "allowKeepingAtDestination": true
    },
    "carReluctance": 2,
    "itineraryFilters" : {
      "bikeRentalDistanceRatio": 0.75,
      "parkAndRideDurationRatio": 0.6,
      "minBikeParkingDistance": 500
    },
    "maxAccessEgressDurationForMode": {
      "CAR_TO_PARK": "30m",
      "BIKE_RENTAL": "30m",
      "BIKE": "30m",
      "WALK": "30m"
    },
    "accessEgress": {
      "penalty": {
        "car-to-park" : { "timePenalty": "10m + 1.5t", "costFactor": 2.5 }
      }
    }
  },
  "transit": {
    "maxNumberOfTransfers" : 6,
    "dynamicSearchWindow" : {
      "minTransitTimeCoefficient" : 0.5,
      "minWindow" : "30m",
      "maxWindow" : "18h"
    }
  },
  "vectorTileLayers": [
    {
      "name": "stops",
      "type": "Stop",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 600,
      "expansionFactor": 0.5
    },
    {
      "name": "stations",
      "type": "Station",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 12,
      "cacheMaxSeconds": 600,
      "expansionFactor": 0.5
    },
    {
      "name": "citybikes",
      "type": "VehicleRental",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60,
      "expansionFactor": 0.5
    },
    // just free-floating vehicles
    {
      "name": "rentalVehicles",
      "type": "VehicleRentalVehicle",
      "mapper": "DigitransitRealtime",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60
    },
    // just rental stations
    {
      "name": "rentalStations",
      "type": "VehicleRentalStation",
      "mapper": "Digitransit",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 600
    },
    // Contains just stations and realtime information for them
    {
      "name": "realtimeRentalStations",
      "type": "VehicleRentalStation",
      "mapper": "DigitransitRealtime",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60
    },
    {
      "name": "parking",
      "type": "VehicleParking",
      "mapper": "Stadtnavi",
      "maxZoom": 20,
      "minZoom": 14,
      "cacheMaxSeconds": 60,
      "expansionFactor": 0.5
    }
  ],
  "updaters": [
    {% if enable_gtfs_rt %}
    {
      "type": "stop-time-updater",
      "url": "https://gtfsr-tripupdate.vvs.de/",
      "feedId": "hbg"
    },
    {
      "type": "real-time-alerts",
      "url": "https://gtfsr-servicealerts.vvs.de/",
      "feedId": "hbg"
    },
    {
      "type": "vehicle-positions",
      "url": "https://gtfsr-vehpos.vvs.de/",
      "feedId": "hbg"
    },
    {
      "type": "stop-time-updater",
      "url": "{{ kvv_gtfs_rt_url }}",
      "feedId": "kvv"
    },
    {
      "type": "stop-time-updater",
      "url": "{{ mfdz_gtfsrt_url }}",
      "feedId": "mfdz"
    },
    {% endif %}
    {
      "type": "bike-rental",
      "frequency": "10m",
      "sourceType": "gbfs",
      "url": "https://backend.open-booking.eu/api/gbfs/2.2/1/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequency": "15m",
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/alf/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequency": "15m",
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/gueltstein-mobil/gbfs.json"
    },
    {
      "type": "bike-rental",
      "sourceType": "gbfs",
      "url": "https://gtfs.mfdz.de/gbfs/regiorad_stuttgart/gbfs.json",
      "allowKeepingRentedVehicleAtDestination": true
    },
    {
      "type": "bike-rental",
      "sourceType": "gbfs",
      "url": "https://platform.tier-services.io/data-sharing/tier_REUTLINGEN/gbfs",
      "headers": {
        "x-api-key": "{{ tier_reutlingen_api_key }}"
      }
    },
    {
      "type": "bike-rental",
      "sourceType": "gbfs",
      "url": "https://platform.tier-services.io/data-sharing/tier_LUDWIGSBURG/gbfs",
      "headers": {
        "x-api-key": "{{ tier_ludwigsburg_api_key }}"
      }
    },
    {
      "type": "bike-rental",
      "sourceType": "gbfs",
      "url": "https://platform.tier-services.io/data-sharing/tier_MUNSINGEN/gbfs",
      "headers": {
        "x-api-key": "{{ tier_munsingen_api_key }}"
      }
    },
    {
      "type": "bike-rental",
      "sourceType": "gbfs",
      "url": "https://{{ api_hostname }}/herrenberg/taxi/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequency": "10m",
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequency": "10m",
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing-teilauto-neckar-alb/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequency": "10m",
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing-stadtmobil-pforzheim/gbfs.json"
    },
    {
      "type": "bike-rental",
      "frequency": "10m",
      "sourceType": "gbfs",
      "url": "https://raw.githubusercontent.com/stadtnavi/static-gbfs-feeds/master/car-sharing-renningen/gbfs.json"
    },
    {% if enable_fake_bike_box -%}
    {
      "type": "vehicle-parking",
      "sourceType": "bicycle-park-api",
      "frequency": "10m",
      "feedId": "open-bike-box",
      "url": "https://api.dev.stadtnavi.eu/herrenberg/parking/open-bike-box.json",
      "tags": ["bike-locker"]
    },
    {% endif %}
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "feedId": "park-api",
      "url": "https://{{ api_hostname }}/herrenberg/parking/parkapi.json"
    },
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "frequency": "10m",
      "feedId": "park-api-rt",
      "url": "https://{{ api_hostname }}/kreis_reutlingen/parking/parkapi.json"
    },
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "frequency": "10m",
      "feedId": "park-api-lbg",
      "url": "https://node21-iot.apps.okd.swlb.de/parkapi.json"
    },
    {
      "type": "vehicle-parking",
      "sourceType": "park-api",
      "frequency": "10m",
      "feedId": "park-api-lbg-bf",
      "url": "https://node21-iot.apps.okd.swlb.de/parkapi_barrierefrei.json"
    }
  ]
}
